<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>newfac</servlet-name>
  	<display-name>newfac</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.new_fac</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>fac_list</servlet-name>
  	<display-name>fac_list</display-name>
  	<description></description>
  	<servlet-class>com.dao.fac_list</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>seefac</servlet-name>
  	<display-name>seefac</display-name>
  	<description></description>
  	<servlet-class>com.admin.servlet.see_fac</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>newfac</servlet-name>
  	<url-pattern>/new_fac/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>fac_list</servlet-name>
  	<url-pattern>/fac_list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>seefac</servlet-name>
  	<url-pattern>/see_fac/*</url-pattern>
  </servlet-mapping>
</web-app>
